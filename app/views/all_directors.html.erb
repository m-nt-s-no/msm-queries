<h1>List of all directors</h1>
<table>
  <tbody>
    <tr>
      <th>ID</th>
      <th>Image</th>
      <th>Name</th>
      <th>Dob</th>
      <th></th>
    </tr>
    <% @dir_array.each do |data| %>
    <tr>
      <% data.each do |datum| %>
        <% if datum.to_s[0..4] == "https" %>
          <td><img src=<%= datum.to_s %>></td>
        <% else%>
          <td><%= datum %></td>
        <% end %>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
